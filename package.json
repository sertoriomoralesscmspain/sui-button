{
  "name": "sui-fw-button",
  "version": "0.0.1",
  "description": "",
  "main": "./src",
  "scripts": {
    "clean:dist": "rm -Rf ./dist/*",
    "predist": "npm run clean:dist -s",
    "dist": "npm run dist:scripts && npm run dist:styles",
    "dist:scripts": "babel src --out-dir dist",
    "dist:styles": "node-sass src/index.scss dist/_button.scss --importer ./scripts/importer.js",
    "postdist:styles": "cp dist/_button.scss dist/button.css",
    "prepublish": "npm run dist",
    "lint": "npm run lint:eslint && npm run lint:sass",
    "lint:eslint": "eslint --ext=\".js\" src/",
    "lint:sass": "scss-lint src/",
    "dev": "npm run dev:open && npm run dev:serve",
    "dev:serve": "webpack-dev-server --port 3000 --devtool eval --progress --colors --content-base doc/",
    "dev:open": "opener http://localhost:3000",
    "predoc": "webpack --config webpack.doc.config.js",
    "doc": "npm run doc:build",
    "doc:build": "gh-pages -d docs"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@schibstedspain/frontend-pre-commit-rules": "^6.2.1",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "node-sass": "^3.3.3",
    "opener": "^1.4.1",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
